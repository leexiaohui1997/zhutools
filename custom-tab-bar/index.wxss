.tabbar {
  gap: 48rpx;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 100rpx;
  position: absolute;
  box-sizing: content-box;
  padding-bottom: env(safe-area-inset-bottom);
  display: flex;
  align-items: center;
  justify-content: center;
  pointer-events: auto;
}
.tabbar-item {
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;
  height: 100%;
  max-width: 200rpx;
  color: #17264c;
  font-size: 32rpx;
  opacity: 0.7;
}
.tabbar-item.active {
  opacity: 1;
  font-size: 36rpx;
  font-weight: 500;
}

.tabbar-menu {
  color: #ebca76;
  background: #c24629;
  height: 60rpx;
  width: 100rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 16rpx;
}
